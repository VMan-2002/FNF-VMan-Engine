function beatHit(num:Int) {
	if (num >= 168 && num < 200 && PlayState.instance.camZooming && FlxG.camera.zoom < 1.35) {
		FlxG.camera.zoom += 0.015;
		PlayState.instance.camHUD.zoom += 0.03;
	}
}